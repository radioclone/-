# VorteX Environment Configuration
VITE_APP_NAME=VorteX
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Web3 Configuration
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Web3Auth Configuration
VITE_WEB3AUTH_CLIENT_ID=your_web3auth_client_id

# Startale/Soneium Configuration
VITE_SONEIUM_RPC_URL=https://soneium-minato.rpc.scs.startale.com?apikey=your_api_key
VITE_SONEIUM_WSS_URL=wss://soneium-minato.rpc.scs.startale.com?apikey=your_api_key
VITE_STARTALE_API_KEY=your_startale_api_key

# Chain Configuration (Soneium Minato Testnet)
VITE_DEFAULT_CHAIN_ID=1946
VITE_CHAIN_NAME=Soneium Minato Testnet

# Features
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_SMART_ACCOUNTS=true
VITE_ENABLE_GASLESS_TX=true
VITE_ENABLE_MULTI_CHAIN=true
